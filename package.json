{
  "name": "pressmedics",
  "version": "0.9.1",
  "contributors": [
    "Polymer and Pressmedics Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "hyperpress/pressmedics",
  "scripts": {
    "start": "es-dev-server --app-index index.html --hostname 192.168.1.45 --port 8803 --node-resolve --watch --open",
    "start:compatibility": "es-dev-server --compatibility all --app-index index.html --node-resolve --watch --open",
    "build": "rimraf dist && rollup -c rollup.config.js && workbox generateSW workbox-config.js",
    "build:rollup": "rimraf && rollup -c && gulp generate-service-worker && gulp rollup-move ",
    "build:static": "polymer build",
    "serve": "prpl-server --root server/build",
    "serve:rollup": "polymer serve --port 3000 public",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@open-wc/building-rollup": "0.15.4",
    "@polymer/app-layout": "3.1.0",
    "@polymer/polymer": "3.3.1",
    "@power-elements/lazy-image": "2.1.1",
    "date-fns": "2.8.1",
    "install": "0.13.0",
    "lit-element": "2.2.1",
    "pwa-helpers": "0.9.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "rollup-plugin-modulepreload": "1.2.2",
    "systemjs": "6.1.7",
    "workbox-build": "4.3.1",
    "workbox-cli": "4.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/plugin-syntax-import-meta": "7.7.4",
    "@babel/preset-env": "7.7.6",
    "@polymer/test-fixture": "4.0.2",
    "axe-core": "3.4.0",
    "chai": "4.2.0",
    "del": "5.1.0",
    "es-dev-server": "1.31.1",
    "eslint": "6.7.2",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-lit": "1.2.0",
    "gulp": "4.0.2",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.0.0",
    "http-server": "0.12.0",
    "jshint": "2.10.3",
    "mocha": "6.2.2",
    "pixelmatch": "5.1.0",
    "polymer-cli": "1.9.11",
    "polyserve": "0.27.15",
    "prpl-server": "1.4.0",
    "puppeteer": "2.0.0",
    "rollup": "1.27.10",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-cpy": "2.0.1",
    "rollup-plugin-minify-html-literals": "1.2.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.3",
    "wct-mocha": "1.0.1"
  }
}
